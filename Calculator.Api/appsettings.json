{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "CalculatorLog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "calculator.log",
          "outputTemplate": "{Message}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
